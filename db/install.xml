<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="auth/shibboleth_link/db" VERSION="2018110200" COMMENT="XMLDB file for Moodle auth/shibboleth_link"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
<TABLES>
    <TABLE NAME="auth_shibboleth_link" COMMENT="Link shibboleth users to moodle users">
        <FIELDS>
            <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"/>
            <FIELD NAME="idp" TYPE="char" LENGTH="255" NOTNULL="true" COMMENT="The identity provider id" />
            <FIELD NAME="idpusername" TYPE="char" LENGTH="255" NOTNULL="true" COMMENT="The username from the identity provider id" />
            <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="The moodle userid" />
            <FIELD NAME="lastseen" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" />
            <FIELD NAME="created" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" />
        </FIELDS>
        <KEYS>
            <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for auth_shibboleth_link"/>
        </KEYS>
    </TABLE>
</TABLES>
</XMLDB>
